  "dependencies": {
    "express": "^4.13.3",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "5.2.9",
