{
  "name": "triple-triad",
  "version": "1.0.0",
  "description": "implementation of final fantasy 8's card game for the web",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "NODE_ENV=development babel-node src/server/index.js --progress --colors --profile",
    "build": "NODE_ENV=production npm run clean && webpack -p --config webpack.production.config.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive",
    "test:unit": "NODE_ENV=test mocha ./test/unit/ --compilers js:babel-core/register --recursive",
    "test:integration": "NODE_ENV=test PORT=4000 mocha ./test/integration/ --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test:unit -- --watch",
    "deploy": "git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/walkerrandolphsmith/triple-triad.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/walkerrandolphsmith/triple-triad/issues"
  },
  "homepage": "https://github.com/walkerrandolphsmith/triple-triad",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.14.2",
    "bootstrap": "^3.3.6",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "immutable": "^3.7.5",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-cors": "0.0.16",
    "koa-passport": "^1.2.0",
    "koa-router": "^5.3.0",
    "koa-static": "^2.0.0",
    "mailgun-js": "^0.7.7",
    "mongoose": "^4.3.6",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.2",
    "reselect": "^2.0.1",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "babel-template": "^6.3.13",
    "babel-types": "^6.4.1",
    "cookie-session": "^1.2.0",
    "css-loader": "^0.23.1",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.12.3",
    "supertest": "^1.1.0",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.11.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          "stage-0",
          "es2015",
          "react"
        ]
      },
      "development": {
        "presets": [
          "stage-0",
          "es2015",
          "react"
        ]
      },
      "test": {
        "presets": [
          "stage-0",
          "es2015",
          "react"
        ],
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "engines": {
    "node": "4.1.1"
  }
}
