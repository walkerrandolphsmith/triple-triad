{
  "name": "triple-triad",
  "version": "1.0.0",
  "description": "implementation of final fantasy 8's card game for the web",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./runner.js",
    "dev": "npm run start & webpack-dev-server --progress --color",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.config.js",
    "deploy": "git push heroku master",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch"
  },
